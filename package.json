{
  "name": "phosphor-webcomponent-build",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "example",
    "packages/*"
  ],
  "scripts": {
    "prebuild": "lerna exec -- rimraf dist",
    "build": "yarn build:dev && yarn build:prod",
    "build:dev": "webpack",
    "build:prod": "yarn build:dev --env=production",
    "postbuild": "lerna exec --scope=!phosphor-layout-examples -- cp ../../README.md README.md",
    "example": "npm-run-all --print-label example:package example:example",
    "example:package": "npm run build:dev --watch",
    "example:example": "webpack-dev-server --config ./example/webpack.config.ts",
    "example:build": "webpack --config ./example/webpack.config.ts",
    "lint": "tslint --project tsconfig.json",
    "prepublishOnly": "yarn build",
    "test": "karma start",
    "test:ci": "yarn test --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukeSheard/phosphor-webcomponent.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukeSheard/phosphor-webcomponent/issues"
  },
  "homepage": "https://github.com/LukeSheard/phosphor-webcomponent#readme",
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/jasmine": "^2.8.3",
    "@types/node": "^8.0.54",
    "@types/webpack": "^3.8.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-webpack": "^2.0.9",
    "lerna": "^2.5.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "tslib": "^1.8.1"
  }
}
