{
    "rulesDirectory": ["tslint-plugin-prettier"],
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "prefer-for-of": false,
        "prettier": [true, {
            "printWidth": 120
        }],
        "member-ordering": [
            true,
            {
                "order": "statics-first"
            }
        ],
        "variable-name": false
    }
}